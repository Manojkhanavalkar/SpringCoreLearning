<?xml version="1.0" encoding="UTF-8"?>
<beans  xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns:context="http://www.springframework.org/schema/context"
xmlns:p="http://www.springframework.org/schema/p"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
http://www.springframework.org/schema/context
http://www.springframework.org/schema/context/spring-context.xsd"
>
<!-- this is our beans-->
<bean name="emp1" class="com.springcore.collections.Emp">
	<property name="name" value="Radhe"/>
	<property name="phones">
		<list>
			<value>123456789</value>
			<value>23745633</value>
			<value>466734</value>
			
		</list>
	</property>
	<property name="address">
		<set>
			<value>Delhi</value>
			<value>Pune</value>
			<value>Mumbai</value>
			<value>Satara</value>
		</set>
	</property>
	<property name="courses">
		<map>
			<entry key="java" value="2month"/>
			<entry key="python" value="1month"/>
			<entry key="Devops" value="9months"/>
		</map>
	</property>
	<property name="dataBase">
		<props>
			<prop key="DBname">cognizant</prop>
			<prop key="password">Raju123</prop>
		</props>
	</property>
</bean>
</beans>